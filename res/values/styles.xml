<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style parent="android:TextAppearance.Medium" name="Bubble.TextAppearance.Dark">

    <item name="android:textSize">14sp</item>

    <item name="android:textColor">#ff7f7f7f</item>

</style>


    <style parent="Bubble.TextAppearance.Dark" name="Bubble.TextAppearance.Light">

    <item name="android:textColor">#ffeeeeee</item>

</style>


    <style parent="android:TextAppearance" name="ClusterIcon.TextAppearance">

    <item name="android:textColor">#ffeeeeee</item>

    <item name="android:textSize">16sp</item>

    <item name="android:textStyle">bold</item>

</style>

    <style name="iconGenText_WHITE">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <style name="iconGenText_BLACK">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="DashboardButton">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:drawablePadding">2dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">@dimen/text_size_small</item>
    </style>

</resources>