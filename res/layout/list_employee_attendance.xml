<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="horizontal"
              android:gravity="center_vertical"
              android:background="@drawable/rowselector"
              android:padding="3dp"
        >

    <com.parse.ParseImageView
            android:id="@+id/list_employee_attendance_iv_photo"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:background="#CCCCCC"
            />
    <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical">
        <TextView
                android:id="@+id/list_employee_attendance_tv_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:textSize="24sp"
                android:textColor="@color/bbutton_primary"
                android:text="@string/notFoundError"/>
        <TextView
            android:id="@+id/list_employee_attendance_tv_so_anh_chup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginStart="10dp"
            android:text="@string/notFoundError"
            android:layout_gravity="bottom"/>
        <TextView
                android:id="@+id/list_employee_attendance_tv_so_cua_hang_vieng_tham"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:text="@string/notFoundError"
                android:layout_gravity="bottom"/>
    </LinearLayout>



</LinearLayout>